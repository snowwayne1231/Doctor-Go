{
    "name": "com.beauty.godoctor.snow",
    "version": "1.0.0",
    "description": "Brand new cordova project with framework7 - vue - webpack!",
    "scripts": {
        "default": "echo \"There is no scripts you can run with npm run command.\" && exit 1",
        "predev": "SET NODE_ENV=development",
        "dev": "cordova run browser -- --lr",
        "dev-p": "cordova run browser --release --lr",
        "dev-android": "cordova run android --release",
        "preandroid": "SET NODE_ENV=product",
        "android": "cordova build android --release",
        "prebuild": "SET NODE_ENV=product",
        "build": "cordova build browser --release"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "license": "ISC",
